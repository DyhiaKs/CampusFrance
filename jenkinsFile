            }
        }
        stage('Build') {
            steps {
           
                bat '"C:\\Program Files\\dotnet\\dotnet.exe" build C:\\Users\\dyhia\\source\\repos\\SpecFlowBDDCampus\\SpecFlowBDDCampus' // Utilisation de 'bat' pour exécuter des commandes Windows
               

            }
